.icon-arrow-right-copy-copy:before {
  content: "\e603";
}


.aboutus_body{
  padding: 0 12px;
}
.about_logo {
  width: 100%;
  padding-top: 20px
}
.about_logo .logo{
  display: block;
  width: 70px;
  height: 70px;
  margin: 40px auto 0;
}
.about_logo text{
  display: block;
  font-weight: 600;
  font-size: 18px;
  color: #000;
  text-align: center;
  line-height: 54px;
  margin-bottom: 20px;
}
.aboutus_list{
  padding: 0 12px;
  box-sizing: border-box;
}
.aboutus_list_li{
  width: 100%;
  height: 44px;
  line-height: 44px;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  border-bottom: 2px solid #fafafa;
}
.aboutus_list_li_left{
  font-size: 14px;
  color: #11101c;
  font-weight: 600;
}
.aboutus_list_li_right{
  color: #9d9d9d;
  font-size: 14px;
  display: flex;
}
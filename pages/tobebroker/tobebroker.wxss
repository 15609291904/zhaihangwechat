.body{
  position: relative;
}

.tobebroker_body{
  padding: 12px;
  height: calc(100% - 64px);
  overflow-x: hidden;
  overflow-y: scroll;
}
.tobebroker_header{
  color: #020202;
  font-size: 18px;
  margin-bottom: 18px;
  font-weight: 600;
}
.messcard{
  box-shadow: 0 4px 6px #e2e1e1;
  padding: 16px;
  margin-bottom: 20px;
  border-radius: 6px;
}
.messcard_title{
  display: flex;
  align-items: center;
  margin-bottom: 12px;
}
.line{
  width: 8px;
  height: 2px;
  background: #f6a702;
  margin-right: 6px;
}
.card_title{
  font-size: 16px;
  color: #000;
  font-weight: 600;
}
.selectcity{
  width: 319px;
  height: 32px;
  padding: 0 20px;
  box-sizing: border-box;
  border-radius: 20px;
  background: #f8f8f8;
  color: #f6a702;
  font-size: 14px;
  border-radius: 20px;
  border: 0;
  outline: none;
}
.selectcompany{
  display: flex;
  flex-direction: column;
}
.selectradio{
  display: flex;
  align-items: center;
  margin-bottom: 16px;
}
.selectcompany_radio{
    border: 1px solid #f7a803;
    width: 17px;
    height: 17px;
    margin-right: 5px;
    position: relative;
    border-radius: 50%;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.selectcompany_radio_checked {
    font-size: 0;
    width: 10px;
    height: 10px;
    background: #f7a803;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -5px;
    margin-top: -5px;
    /* display: none; */
    border-radius: 50%;
}
.selectradio text{
  font-size: 13px;
  color: #898989;
}
.shopselect{
  display: flex;
  justify-content: center;
  font-size: 14px;
  display: flex;
  width: 136px;
  height: 30px;
  border-radius: 20px;
  background: #f7a803;
  text-align: center;
  line-height: 30px;
  color: #fff;
}
.jobcard_box{
  display: flex;
  flex-direction: column;
}
.jobcard{
  background: #f8f8f8;
  width: 320px;
  height: 200px;
  margin-bottom: 10px;
  border-radius: 6px;
  position: relative;
}
.jobcard image{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.uploadimg_card{
  z-index: 99;
}
.uploadicon{
  display: block;
  width: 46px;
  height: 46px;
  font-size: 46px;
  color: #f7b226;
  position: absolute;
  left: 0 ;
  top: 0 ;
  right: 0 ;
  bottom: 0;
  margin: auto;
  z-index: 11;
}
.uploadimg_title{
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  font-size: 22px;
  background: #f7b226;
  border-bottom-left-radius: 6px;
  border-bottom-right-radius: 6px;
  line-height: 46px;
  text-align: center;
  z-index: 999;
  color: #fff;
}
.agreementbtn{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.agreementbtn_checked{
  display: flex;
  align-items: center;
  font-size: 15px;
  color: #909090;
}
.agreementbtn_checked image{
  width: 20px;
  height: 20px;
}
.broker_agreement{
  font-size: 15px;
  color: #f6b966;
}
.success_broker{
  margin: 40px auto 70px;
  width: 200px;
  height: 35px;
  line-height: 35px;
  text-align: center;
  color: #fff;
  background: #f6a702;
  border-radius: 20px;
  border: 0;
  outline: none;
}

.checkbox_body{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.checkbox{
  width: 270px;
  height: 348px;
  background: #fff;
  border-radius: 14px;
  margin: 110px auto 0;
  padding: 42px 35px 50px;
  box-sizing: border-box;
}
.checkimages{
  width: 172px;
  height: 115px;
  margin: 0 14px 14px;
}
.checkbox text{
  display: block;
}
.checkbox_title1{
  font-size: 16px;
  color: #4b4b4b;
  line-height: 28px;
  margin-bottom: 8px;
  text-align: center;
}
.checkbox_title2{
  font-size: 12px;
  color: #f8b245;
  margin-bottom: 24px;
  text-align: center;
}
.backtoprev{
  font-size: 16px;
  text-align: center;
  color: #fff;
  background: #f6a702;
  width: 100%;
  height: 36px;
  line-height: 36px;
  border-radius: 20px;
  box-shadow: 0 4px 8px #f3ca8e;
}
.closebtn_toprev{
  font-size: 40px;
  color: #fff;
  margin: 10px auto 0;
  text-align: center;
}





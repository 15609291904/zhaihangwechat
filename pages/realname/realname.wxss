.icon-tianjia-yin:before {
  content: "\e68f";
}

.icon-guanbi:before {
  content: "\e608";
}


.body{
  display: flex;
  flex-direction: column;
  position: relative;
}
.realname_body{
  padding: 12px;
}
.realname_namecard,.realname_numbercard,.messCard{
  border-radius: 6px;
  padding: 16px;
  margin-bottom: 20px;
  box-sizing: border-box;
  box-shadow: 0 4px 6px #e2e1e1;
}
.name_card{
  display: flex;
  align-items: center;
  margin-bottom: 12px;
}
.line{
  width: 8px;
  height: 2px;
  background: #f6a702;
  margin-right: 6px;
}
.name_card_title{
  font-size: 16px;
  color: #000;
  font-weight: 600;
}
.name_card_input,.number_card_input{
  padding: 0 20px;
  height: 32px;
  border-radius: 20px;
  background: #f8f8f8;
  color: #f6a702;
  border: 0;
  outline: none;
  font-size: 14px;
}
.uploadbox{
  display: flex;
  flex-direction: column;
  margin-bottom: 6px;
}
.uploadimg{
  width: 100%;
  height: 200px;
  position: relative;
  border-radius: 6px;
  margin-bottom:10px;
}
.uploadimg image{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0 ;
  top: 0;
}
.uploadimg .uploadimg_bg{
  z-index: 10;
}
.realname_img1{
  background: #f8f8f8;
}
.uploadimg_card{
  z-index: 999;
}
.uploadicon{
  display: block;
  width: 46px;
  height: 46px;
  font-size: 46px;
  color: #f7b226;
  position: absolute;
  left: 0 ;
  top: 0 ;
  right: 0 ;
  bottom: 0;
  margin: auto;
  z-index: 11;
}
.uploadimg_title{
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  font-size: 22px;
  background: #f7b226;
  border-bottom-left-radius: 6px;
  border-bottom-right-radius: 6px;
  line-height: 46px;
  text-align: center;
  z-index: 999;
  color: #fff;
}
.uploadAttachWord{
  color: #444444;
  font-size: 12px;
  line-height: 22px;
  text-align: center;
}
.uploadAttachWord text{
  color: #f7b226;
}
.fabuButton{
  font-size: 16px;
  text-align: center;
  color: #fff;
  background: #f6a702;
  width: 100%;
  height: 36px;
  line-height: 36px;
  border-radius: 20px;
  box-shadow: 0 4px 8px #f3ca8e;
  margin: 40px auto 70px;
  width: 200px;
  border: 0;
  outline: none;
}

.checkbox_body{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.checkbox{
  width: 270px;
  height: 348px;
  background: #fff;
  border-radius: 14px;
  margin: 110px auto 0;
  padding: 42px 35px 50px;
  box-sizing: border-box;
}
.checkimages{
  width: 172px;
  height: 115px;
  margin: 0 14px 14px;
}
.checkbox text{
  display: block;
}
.checkbox_title1{
  font-size: 16px;
  color: #4b4b4b;
  line-height: 28px;
  margin-bottom: 8px;
  text-align: center;
}
.checkbox_title2{
  font-size: 12px;
  color: #f8b245;
  margin-bottom: 24px;
  text-align: center;
}
.backtoprev{
  font-size: 16px;
  text-align: center;
  color: #fff;
  background: #f6a702;
  width: 100%;
  height: 36px;
  line-height: 36px;
  border-radius: 20px;
  box-shadow: 0 4px 8px #f3ca8e;
}
.closebtn_toprev{
  font-size: 40px;
  color: #fff;
  margin: 10px auto 0;
  text-align: center;
}
/* pages/orderdetail/orderdetail.wxss */
@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1559116294972'); /* IE9 */
  src: url('iconfont.eot?t=1559116294972#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('iconfont.woff?t=1559116294972') format('woff'),
  url('iconfont.ttf?t=1559116294972') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('iconfont.svg?t=1559116294972#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-mingpian:before {
  content: "\e753";
}


.orderdetail_body{
  padding: 0 12px;
}
.orderdetail_title{
  padding: 0 10px;
  border-left: 2px solid #faa600;
  color: #faa600;
  font-size: 14px;
  font-weight: 600;
  line-height: 13px;
  margin-bottom: 10px;
}
.messcard{
  box-shadow: 0 4px 6px #e2e1e1;
  padding: 16px;
  margin-bottom: 20px;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  border-radius: 6px;
}
.messcard_left{
  width: 20%;
}
.messcard_img{
  width: 60px;
  height: 60px;
  border-radius: 50%;
}
.messcard_center{
  width: 60%;
  padding: 0 10px;
  box-sizing: border-box;
}
.messcard_right{
  width: 20%;
}
.messcard_center1{
  display: flex;
  margin-bottom: 8px;
}
.broker_name{
  font-size: 16px;
  font-weight: 700;
  color: #000;
  line-height: 22px;
}
.mingpian{
  font-size: 20px;
  color: #f8a501;
  margin: 0 8px;
  line-height: 22px;
}
.score{
  font-size: 14px;
  color: #f8a501;
}
.bio{
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 12px;
  color: #959595;
}
.messcard_right_money{
  font-size: 14px;
  color: #f59500;
}
.messcard_right .money_num{
  font-size: 22px;
}
.messcard_right_title{
  font-size: 12px;
  color: #909090;
  line-height: 20px;
}
.messcard2{
  justify-content: space-between;
}
.casedetail_info_price1{
  text-align: center;
}
.casedetail_info_price1 view{
  font-size: 25px;
  color: #F6A802;
}
.casedetail_info_price1 text,.casedetail_info_price2 text,.casedetail_info_price3 text{
  font-size: 12px;
  color: #2B2B2B;
  /* font-weight: 600; */
}
.casedetail_line{
  width: 1px;
  height: 25px;
  background: #979797;
  opacity: .1;
  margin-top: 16px;
}
.casedetail_info_price2 view{
  font-size: 30px;
  color: #F6A802;
  line-height: 33px;
}
.casedetail_info_time{
  font-size: 30px;
  color: #F6A802;
  line-height: 21px;
}
.casedetail_info_time text{
  font-size: 15px;
  color: #F6A802;
}
.loanDetialList{
  padding: 12px;
  box-sizing: border-box;
  display: flex;
}
.loanDetialListLeft{
  margin-right: 20px;
}
.loanDetialList text{
  display: block;
  font-size: 12px;
  line-height: 24px;
}
.loanDetialListLeft text{
  color:#989898;
}
.loanDetialListRight text{
  color: #4d4d4d;
}

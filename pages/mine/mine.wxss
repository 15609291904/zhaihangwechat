.mine_body{
  padding: 0 12px;
  background: #fff;
}
.mine_nologin_header{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.mine_nologin_header_left{
  display: flex;
}
.mine_nologin_header image{
  width: 62px;
  height: 62px;
  margin-right: 13px;
  border-radius: 50%;
}
.mine_nologin_header .tologin{
  font-size: 18px;
  color: #2B2B2B;
  line-height: 62px;
}
.mine_nologin_header_right,.mine_login_header_info_right{
  display: flex;
  font-size: 11px;
  color: #CFCFCF;
  line-height: 62px;
}
.mine_login_header{
  display: flex;
  width: 100%;
  margin-bottom: 20px;
}
.mine_login_header_head{
  width: 62px;
  height: 62px;
  margin-right: 16px;
  border-radius: 50%;
}
.mine_login_header_head image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.mine_login_header_info{
  display: flex;
  justify-content: space-between;
  flex: 1;
}
.mine_login_header_info_left{
  display: flex;
  flex-direction: column;
}
.mine_login_header_infophone{
  font-size: 20px;
  color: #0c0c0c;
  margin-bottom: 8px;
}
.mine_login_header_inforenz{
  display: flex;
  line-height: 12px;
}
.mine_login_header_inforenz_vip{
  width: 12px;
  height: 12px;
  margin-right: 4px;
}
.login_renz{
  font-size: 12px;
  color: #f59000;
}
.login_city{
  font-size: 12px;
  color: rgb(156, 156, 156);
  margin-left: 10px;
}


.billCard{
  width: 100%;
  height: 75px;
  background: rgba(246,168,2,1);
  box-shadow: 2.5px 4.33px 10px 0px rgba(227,130,4,0.5);
  border-radius: 5px;
  margin: 20px 0 20px 0;
}
.billCard{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.billCardli{
  /* height: 100%; */
  display: flex;
  flex-direction: column;
  text-align: center;
  padding: 12px 0;
  box-sizing: border-box;
}
.billCardli_num{
  font-size: 16px;
  color: white;
  margin-bottom: 6px;
}
.billCardli_title{
  display: block;
  font-size: 14px;
  color: white;
}
.minelist{
  width: 100%;
  padding: 0 12px;
  box-sizing: border-box;
}
.mine_li{
  display: flex;
  justify-content: space-between;
  padding: 11px 0;
  box-sizing: border-box;
  border-bottom: 2px solid#fafafa;
}
.mine_li_left{
  display: flex;
}
.mine_li_lefticon{
  font-size: 20px;
  /* color: #f7a400; */
  margin-right: 10px;
}
.mine_li_left text{
  font-size: 14px;
  color: #11101c;
}
.mine_li_right{
  display: flex;
}
.renzheng{
  font-size: 14px;
  color: #9d9d9d;
}
.mine_li_righticon{
  color: #B1B1B1;
}
.mine_claim{
  color:  #f7a400; 
}
.claim_about{
  color: #9c9aee;
}
.broker_renz{
  color:#82e0c0;
}
.about_us{
  color: #57a2f8;
}
.request_broker{
  color: #82e0c0;
}
.logout{
  width: 200px;
  height: 35px;
  background: #f6a702;
  color:#fff;
  line-height: 35px;
  text-align: center;
  font-size: 14px;
  border-radius: 20px;
  border: 0;
  outline: none;
  margin: 40px auto 70px;
}